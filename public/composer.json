{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"repositories": [
		{
			"type": "vcs",
			"url": "git@bitbucket.org:titledk/silverstripe-calendar.git"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"silverstripe/cms": "3.1.5",
		"silverstripe/framework": "3.1.5",
		"silverstripe-themes/simple": "*",
		"titledk/silverstripe-calendar": "dev-dev",
		"unclecheese/bootstrap-forms": "dev-master"
	},
	"config": {
		"process-timeout": 600	
	},
	"minimum-stability": "dev"
}
